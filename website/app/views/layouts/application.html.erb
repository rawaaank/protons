    <!-- ################################################################################################ -->
  </header>
</div>
<div class="wrapper row4">
  <nav id="mainav" class="hoc clear">
    <!-- ################################################################################################ -->
    <ul class="clear">
      <li class="active"><a href="/main/index">Home</a></li>
      <li><a href="/main/gallery">Gallery</a>
      </li>
      <li><a class="drop" href="#">Tracks</a>
        <ul>
          <li><a href="/main/robotics">Robotics</a></li>
          <li><a href="/main/programming"> Programming </a></li>
          <li><a href="/main/automation">Automation</a></li>
        </ul>
      </li>
      <% unless current_user %>
      <li><a href="/users/sign_up">Join Us</a></li>
      <% end %>

